{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "framework-j1-adf"
		},
		"Metadata Database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Metadata Database'"
		},
		"Azure Key Vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sjp-carbon-vault.vault.azure.net/"
		},
		"Framework Databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0128-132544-vg0vau3q"
		},
		"Metadata Database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "framework-j1-sv.database.windows.net"
		},
		"Metadata Database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "framework-j1-db"
		},
		"Metadata Database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsjp"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "framework-j1-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Metadata Database_password": {
						"value": "[parameters('Metadata Database_password')]"
					},
					"Azure Key Vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('Azure Key Vault_properties_typeProperties_baseUrl')]"
					},
					"Metadata Database_properties_typeProperties_server": {
						"value": "[parameters('Metadata Database_properties_typeProperties_server')]"
					},
					"Metadata Database_properties_typeProperties_database": {
						"value": "[parameters('Metadata Database_properties_typeProperties_database')]"
					},
					"Metadata Database_properties_typeProperties_userName": {
						"value": "[parameters('Metadata Database_properties_typeProperties_userName')]"
					},
					"Framework Databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Framework Databricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}